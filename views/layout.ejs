    <title>
        <%= titlePage %>
    </title>
    <!-- Bootstrap -->
    <link href="/static/css/bootstrap.min.css" rel="stylesheet">
    <link href="/static/css/jquery-ui.css" rel="stylesheet">
    <link href="/static/css/jquery.desoslide.css" rel="stylesheet">
