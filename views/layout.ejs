<body>
    <!-- top-header-->
    <div class="top-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-7 col-sm-6 hidden-xs">
                    <p class="top-text">Flexible Delivery, Fast Delivery.</p>
                </div>
                <div class="col-lg-4 col-md-5 col-sm-6 col-xs-12">

                </div>
            </div>
            <!-- /.top-header-->
        </div>
    </div>
    <!-- header-section-->
    <div class="header-wrapper">
        <div class="container">
            <div class="row">
                <!-- logo -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-8" style="top: 14px;" id="colTitulo">

                    <h2 class="quailstore">QUAIL STORE</h2>

                </div>
                <!-- /.logo -->
                <!-- search -->
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="search-bg">
                        <form action="/produto/busca/filtrar" method="post">
                            <input type="text" class="form-control" name="buscar"
                                placeholder="Digite o que você procura...">
                            <button type="Submit"><i class="fa fa-search"></i></button>
                        </form>
                    </div>
                </div>
                <!-- /.search -->
                <!-- account -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                    <% if (user==undefined) { %>
                        <div class="account-section">
                            <ul>
                                <li><a href="/user/login" class="title hidden-xs">entrar</a></li>
                                <li class="hidden-xs">|</li>
                                <li><a href="/user/criar-conta" class="title hidden-xs">Criar conta</a></li>
                                <li><a href="/carrinho" class="title"><i class="fa fa-shopping-cart"></i> <sup
                                            class="cart-quantity">
                                            <%= carrinho.length %>
                                        </sup></a>
                                </li>
                            </ul>
                        </div>
                        <% } else { %>
                            <div class="account-section">
                                <ul>
                                    <li><a href="/pedido/meus-pedidos" class="title "><i
                                                class="glyphicon glyphicon-user"></i>
                                            <%= user.nome %>
                                        </a></li>
                                    <li><a href="/carrinho" class="title"><i class="fa fa-shopping-cart"></i> <sup
                                                class="cart-quantity">
                                                <%= carrinho.length %>
                                            </sup></a>
                                    </li>
                                </ul>
                            </div>
                            <% } %>
                                <!-- /.account -->
                </div>
                <!-- search -->
            </div>
        </div>
        <!-- navigation -->
        <div class="navigation">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <!-- navigations-->
                        <div id="navigation">
                            <ul>
                                <li class="active"><a href="/">Home</a></li>
                                <li class="has-sub"><a href="#">INFORMÁTICA</a>
                                    <ul>
                                        <li><a href="/produto/categoria/processador">Processador</a></li>
                                        <li><a href="/produto/categoria/ssd">SSD</a></li>
                                        <li><a href="/produto/categoria/memoria-ram">Memória-ram</a></li>
                                        <li><a href="/produto/categoria/placa-mae">Placa-mãe</a></li>
                                    </ul>
                                </li>
                                <li class="has-sub"><a href="#">PERIFÉRICOS</a>
                                    <ul>
                                        <li><a href="/produto/categoria/fone-de-ouvido">FONES DE OUVIDO</a></li>
                                        <li><a href="/produto/categoria/headset">HEADSET</a></li>
                                        <li><a href="/produto/categoria/smartwatch">SMARTWATCH</a></li>
                                        <li><a href="/produto/categoria/mouse">MOUSE</a></li>
                                        <li><a href="/produto/categoria/mouse-pad">MOUSE PAD</a></li>
                                        <li><a href="/produto/categoria/teclado">TECLADO</a></li>
                                        <li><a href="/produto/categoria/teclado+mouse">TECLADO + MOUSE</a></li>
                                    </ul>
                                </li>
                                <li class="has-sub"><a href="#">GEEK</a>
                                    <ul>
                                        <li><a href="/produto/categoria/action-figure">ACTION FIGURE</a></li>
                                        <li><a href="/produto/categoria/card-game">CARD GAME</a></li>
                                        <li><a href="/produto/categoria/funko-pop">FUNKO POP</a></li>
                                        <li><a href="/produto/categoria/lego">LEGO</a></li>
                                        <li><a href="/produto/categoria/miniaturas">MINIATURAS</a></li>
                                        <li><a href="/produto/categoria/pelucias">PELÚCIAS</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- /.navigations-->
                </div>
            </div>
        </div>
    </div>
    <main class="conteudo">
        <div class="container">
            <%- body %>
        </div>
    </main>
    <!-- footer -->
    <div class="footer">
        <div class="container">
            <div class="row">
                <!-- footer-company-links -->
                <!-- footer-contact links -->
                <!-- /.footer-useful-links -->
                <div class=" col-lg-4 col-md-4 col-sm-4 col-xs-12">
                    <div class="footer-widget">
                        <h3 class="footer-title">Links rápidos</h3>
                        <ul class="arrow">
                            <li><a href="/">Home </a></li>
                            <li><a href="/produto/categoria/ssd">SSD</a></li>
                            <li><a href="/produto/categoria/lego">LEGO</a></li>
                            <li><a href="/produto/categoria/processador">PROCESSADOR</a></li>
                        </ul>
                    </div>
                </div>
                <!-- /.footer-useful-links -->
                <!-- footer-policy-list-links -->
                <div class=" col-lg-4 col-md-4 col-sm-4 col-xs-12">
                    <div class="footer-widget">
                        <h3 class="footer-title">Policy Info</h3>
                        <ul class="arrow">
                            <li><a href="#">Payments</a></li>
                            <li><a href="#">Cancellation &amp; Returns</a></li>
                            <li><a href="#">Shipping and Delivery</a></li>
                            <li><a href="#">Privacy Policy</a></li>
                            <li><a href="#">T &amp; C</a></li>
                        </ul>
                    </div>
                </div>
                <!-- /.footer-policy-list-links -->
                <!-- footer-social links -->
                <div class=" col-lg-4 col-md-4 col-sm-4 col-xs-12">
                    <div class="footer-widget">
                        <h3 class="footer-title">Social mídias </h3>
                        <div class="ft-social">
                            <span><a href="#" class="btn-social btn-facebook"><i class="fa fa-facebook"></i></a></span>
                            <!-- <span><a href="#" class="btn-social btn-twitter"><i class="fa fa-twitter"></i></a></span>
                        <span><a href="#" class="btn-social btn-googleplus"><i class="fa fa-google-plus"></i></a></span>
                        <span><a href="#" class=" btn-social btn-linkedin"><i class="fa fa-linkedin"></i></a></span>
                        <span><a href="#" class=" btn-social btn-pinterest"><i class="fa fa-pinterest-p"></i></a></span> -->
                            <span><a href="https://instagram.com/_quailstore?utm_medium=copy_link"
                                    class=" btn-social btn-instagram"><i class="fa fa-instagram"></i></a></span>
                        </div>
                    </div>
                </div>
                <!-- /.footer-social links -->
            </div>
        </div>
        <!-- tiny-footer -->
        <div class="tiny-footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="payment-method alignleft">
                            <ul>
                                <li><a href="#"><i class="fa fa-cc-mastercard  fa-2x"></i></a></li>
                                <li><a href="#"><i class="fa fa-cc-visa fa-2x"></i></a></li>
                                <li><a href="#"><i class="glyphicon glyphicon-barcode fa-2x"></i></a></li>
                            </ul>
                        </div>
                        <p class="alignright">Copyright © All Rights Reserved 2021</p>
                    </div>
                </div>
            </div>
            <!-- /. tiny-footer -->
        </div>
    </div>
    <!-- /.footer -->
